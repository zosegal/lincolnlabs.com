// footer
#footer-wrapper
    float left
    width 100%
    min-height 120px
    background-color #e2e2e2

#footer
    background-color #e2e2e2

#footer .row
    margin-top 30px
    padding-bottom 30px

.footer-headline
    font-size 16px
    color hex-lght-red
    padding-top 5px
    margin-bottom 15px

.footer-a
    width 100%
    display block
    font-size 16px
    color hex-dk-teal
    text-align center

.footer-a:hover
    color hex-lght-yellow
    text-decoration none

#footer-middle-div
    border-left 1px solid hex-dk-teal
    border-right 1px solid hex-dk-teal